@model Student
@{
    ViewBag.Title = "Home | Detail";
}

<p>
    <img src="/Images/favicon.png">
</p>

<table class="table detail">
    <tr>
        <th>Id</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Gender</th>
        <td>@(Model.Gender=="F"?"Female":"Male")</td>
    </tr>
    <tr>
        <th>Program</th>
        <td>@Model.Program.Id-@Model.Program.Name</td>
    </tr>
</table>

<p>
    <button data-get="/">Index</button>
</p>
